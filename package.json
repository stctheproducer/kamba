  "scripts": {
    "start": "dotenvx run -f .env.production --quiet -- node bin/server.js",
    "build": "dotenvx run -f .env.production --quiet -- node ace build",
    "dev": "dotenvx run --quiet -- node ace serve --hmr",
    "test": "dotenvx run --quiet -- node ace test",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "changeset": "changeset",
    "changeset:check": "changeset status --since=main",
    "changeset:empty": "changeset add --empty",
    "version": "changeset version",
    "release": "pnpm build && changeset publish",
    "publish": "changeset publish",
    "email:dev": "email dev -d ./resources/emails",
    "email:build": "email build -d ./resources/emails",
    "email:export": "email export -d ./resources/emails --outDir ./out",
    "email:start": "email start",
    "env:test:set": "dotenvx set -f .env.test --",
    "env:test:get": "dotenvx get -f .env.test --",
    "env:prod:set": "dotenvx set -f .env.production --",
    "env:prod:get": "dotenvx get -f .env.production --",
    "shad:add": "pnpm dlx shadcn@latest add"
  },
  "imports": {
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#mails/*": "./app/mails/*.js",
    "#services/*": "./app/services/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#providers/*": "./providers/*.js",
    "#policies/*": "./app/policies/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#database/*": "./database/*.js",
    "#tests/*": "./tests/*.js",
    "#start/*": "./start/*.js",
    "#config/*": "./config/*.js",
    "#audit_resolvers/*": "./app/audit_resolvers/*.js",
    "#utils/*": "./app/utils/*.js",
    "#emails/*": "./resources/emails/*.js",
    "#drivers/*": "./app/drivers/*.js",
    "#api": "./.adonisjs/index.js",
    "#dtos/*": "./app/dtos/*.js"
  },
